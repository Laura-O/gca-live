{
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "none",
    "svelteStrictMode": true,
    "svelteSortOrder": "options-styles-scripts-markup",
    "svelteAllowShorthand": false,
    "svelteBracketNewLine": true,
    "svelteIndentScriptAndStyle": true,
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}